{
    "name": "phpjim",
    "displayName": "phpjim",
    "author": {
		"name": "jean-michel hinicker",
		"email": "jeanmichelhinicker@gmail.com"
	},
	"publisher": "Hinicker",
    "description": "une extension de snippets pour d√©marrer en php",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.44.0"
    },
    "icon": "minimoi.ico",
	"repository": {
		"url": "https://github.com/JimHin/phpjim"
	},
    "categories": [
        "Snippets"
    ],
    "activationEvents": [
		"onLanguage:html",
		"onLanguage:css",
		"onLanguage:php",
		"onLanguage:javascript"
	],
    "contributes": {
        "snippets": [
            {
                "language": "php",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets_js.json"
            },
            {
                "language": "html",
                "path": "./snippets/snippets_html.json"
            },
            {
                "language": "css",
                "path": "./snippets/snippets_css.json"
            }

        ]
    }
}